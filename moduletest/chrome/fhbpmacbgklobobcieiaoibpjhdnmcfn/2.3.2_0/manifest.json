{
   "action": {
      "default_icon": {
         "16": "icon.png",
         "24": "icon.png",
         "32": "icon.png"
      },
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "contentScript.js" ],
      "matches": [ "https://*.udemy.com/course/*/learn/*", "https://learn.udacity.com/*", "https://www.youtube.com/*" ]
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "options_page": "options.html",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.3.2",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "options.html" ]
   } ]
}
